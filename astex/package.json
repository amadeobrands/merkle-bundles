{
  "name": "merkle-ast-bundles",
  "scripts": {
    "test": "mocha --config ./server.mocha.config.js --compilers js:babel-core/register ./src/**/*.test.js",
    "test:client:unit": "./dist/bin/server.js -d tests/ | karma start client.karma.conf.js",
    "test:integration": "mocha ./tests/e2e/index",
    "build": "babel ./src -d ./dist --ignore test.js --ignore ./src/client/",
    "build-watch": "babel ./src --watch -d ./dist --ignore test.js client/",
    "build:client": "webpack --progress --watch --display-modules --config ./client.webpack.config.js"
  },
  "sideEffects": false,
  "bin": {
    "merkle-ast-serve": "./dist/bin/server.js"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "array-buffer-to-hex": "^1.0.0",
    "body-parser": "^1.18.3",
    "commander": "^2.16.0",
    "compressjs": "^1.0.3",
    "cors": "^2.8.4",
    "deep-diff": "^1.0.1",
    "deep-object-diff": "^1.1.0",
    "es6-promise": "^4.2.4",
    "esprima": "^4.0.0",
    "express": "^4.16.3",
    "get-object-type": "0.0.2",
    "glob": "^7.1.2",
    "hex-to-array-buffer": "^1.1.0",
    "js-sha256": "^0.9.0",
    "json-diff": "^0.5.2",
    "localforage": "^1.7.2",
    "lodash": "^4.17.10",
    "lodash.findindex": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.merge": "^4.6.1",
    "lodash.sortby": "^4.7.0",
    "minimal-object-diff": "^0.1.0",
    "msgpack-lite": "^0.1.26",
    "notepack.io": "^2.1.3",
    "protobufjs": "^6.8.6",
    "puppeteer": "^1.6.0",
    "q": "^1.5.1",
    "qwest": "^4.5.0",
    "recast": "^0.15.0",
    "sinon": "^6.1.3",
    "symbol-description": "^1.0.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.76.0",
    "karma": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "raw-loader": "^0.5.1",
    "static-server": "^2.2.1",
    "supertest": "^3.1.0",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
